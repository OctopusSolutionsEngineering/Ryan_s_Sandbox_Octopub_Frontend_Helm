variable "Octopus.Azure.Account" {
    type = "AzureAccount"

    value "demo-octopus-app" {
        action = ["create-namespace-if-not-exists"]
    }
}

variable "Project.Worker.Pool" {
    type = "WorkerPool"

    value "azure-worker-pool" {}
}

variable "Project.Helm.Replicas" {
    value "1" {}

    value "3" {
        environment = ["production"]
    }
}